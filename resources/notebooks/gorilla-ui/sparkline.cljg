;; **
;;; # Sparkline Demo
;; **

;; **
;;; #  create a vector with random data of size nr
;; **

;; @@ [cljs]
(defn data [nr]
  (let [d (vec (take nr (repeatedly rand)))
       ; _ (println "d is:" d)
        ]
    d
    ))
